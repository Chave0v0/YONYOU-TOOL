<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.chave.controller.ProxyController"
            prefHeight="400.0" prefWidth="600.0">

    <children>
        <CheckBox fx:id="proxyOn" layoutX="50.0" layoutY="57.0" mnemonicParsing="false" text="开启" AnchorPane.leftAnchor="50.0" AnchorPane.topAnchor="40.0" />
        <CheckBox fx:id="proxyOff" layoutX="131.0" layoutY="61.0" mnemonicParsing="false" text="关闭" AnchorPane.rightAnchor="50.0" AnchorPane.topAnchor="40.0" />
        <TextField fx:id="hostField" layoutX="68.0" layoutY="117.0" prefHeight="27.0" prefWidth="135.0" AnchorPane.topAnchor="90.0" />
        <TextField fx:id="portField" layoutX="68.0" layoutY="162.0" prefHeight="27.0" prefWidth="135.0" AnchorPane.topAnchor="145.0" />
        <Label layoutX="28.0" layoutY="95.0" text="Host" AnchorPane.topAnchor="95.0" />
        <Label layoutX="30.0" layoutY="150.0" text="Port" />
        <Button fx:id="submitButton" layoutX="88.0" layoutY="194.0" mnemonicParsing="false" text="确定" AnchorPane.bottomAnchor="30.0" AnchorPane.leftAnchor="55.0" />
        <Button fx:id="cancelButton" mnemonicParsing="false" text="取消" AnchorPane.bottomAnchor="30.0" AnchorPane.rightAnchor="55.0" />
    </children>
</AnchorPane>
